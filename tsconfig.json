{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es2022",
    "target": "es2022",
    "allowJs": false,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strict": true,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "noImplicitOverride": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    // "noUnusedParameters": true,
  },
  "include": ["**/*.ts", "**/*.tsx", "webpack.config.js"]
}
